modules = ["nodejs-18"]

# Main run command for development
run = ["npm", "run", "dev"]

[deployment]
build = ["npm", "install", "--legacy-peer-deps", "&&", "npm", "run", "build"]
run = ["npm", "run", "start:replit"]

[[ports]]
localPort = 5000
externalPort = 80

[env]
NODE_ENV = "development"

[agent]
expertMode = true
