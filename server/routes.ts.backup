          // Skip low confidence or generic items
          if (confidence < 0.2) {
            console.log(`  ‚ö†Ô∏è Skipping low confidence: ${confidence}`);
            continue;
          }
          if (foodName.toLowerCase().includes('unknown')) {
            console.log(`  ‚ö†Ô∏è Skipping unknown item`);
            continue;
          }
          
          const unit = getUnitForFood(foodName);
          
          detectedIngredients.push({
            id: `food-${Date.now()}-${Math.random()}`,
            name: foodName,
            confidence: confidence,
            amount: 1,
            unit: unit,
            measureType: getMeasureType(unit),
            source: 'dish'
          });
          
          console.log(`  ‚úÖ Added dish: ${foodName} (${(confidence * 100).toFixed(1)}%)`);
        }
      } else {
        console.log('‚ö†Ô∏è No recognition_results in response');
      }
      
